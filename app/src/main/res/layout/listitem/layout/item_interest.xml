<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    android:gravity="center_horizontal"
    android:id="@+id/item_interest"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center_horizontal"
    android:layout_centerVertical="true"
    android:layout_marginBottom="@dimen/space_50px"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:layout_marginRight="@dimen/space_20px"
        android:layout_marginLeft="@dimen/space_20px"
        android:layout_gravity="center_horizontal"
        android:textColor="@color/text_select_color"
        android:textSize="@dimen/space_30px"
        android:background="@drawable/checkbox_unselect"
        android:id="@+id/item_interst_tv"
        android:button="@null"
        android:text="数码"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</com.zhy.autolayout.AutoLinearLayout>